<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" prefWidth="402.0" spacing="10" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.projeto.projetoFabinho.Controllers.CarList.CarListController">
    
    <TextField fx:id="searchField" prefWidth="402.0" promptText="Pesquisar Veículo..." />
    
    <TableView fx:id="tableView">
        <columns>
            <TableColumn fx:id="colMarca" prefWidth="86.0" text="Marca" />
            <TableColumn fx:id="colModelo" prefWidth="67.0" text="Modelo" />
            <TableColumn fx:id="colPlaca" prefWidth="96.0" text="Placa" />
            <TableColumn fx:id="colAnoFabricacao" prefWidth="57.0" text="Ano/Fab" />
            <TableColumn fx:id="colSituacao" prefWidth="75.0" text="Situação" />
        </columns>
    </TableView>

    <Button fx:id="selecionarButton" text="Selecionar Veículo" />
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</VBox>
